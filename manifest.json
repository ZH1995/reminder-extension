{
    "manifest_version": 3,
    "name": "Reminder Extension",
    "version": "1.0",
    "description": "提醒你完成每日任务的Chrome扩展。",
    "permissions": ["storage"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon16.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        }
    },
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    }
}
